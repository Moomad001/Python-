# โค้ดคำนวณค่าน้ำ
water = float(input('ปริมาณน้ำที่่ใช้:'))
meter = input('ขนาดมาตรวัดน้ำ:')
tax = (7/100)
D = (10.20)
E = (16.00)
F = (19.00)
G = (21.20)
print ('ขนาดมาตรวัดน้ำ, %s' %meter,"นิ้ว") 
if water > 0 and water < 11: 
  if meter == "1/2":
    A= (water*D)+30
    B= (A)*tax
    C= B+A       
    print ('ราคาไม่รวมภาษี, %s' %A,"บาท")
    print ('ราคาภาษี, %s' %B,"บาท")
    print ('ราคารวมทั้งหหมด, %s' %C,"บาท")
if water > 10 and water < 21: 
  if meter == "1/2":
    A= (water*E)+30
    B= (A)*tax
    C= B+A       
    print ('ราคาไม่รวมภาษี, %s' %A,"บาท")
    print ('ราคาภาษี, %s' %B,"บาท")
    print ('ราคารวมทั้งหหมด, %s' %C,"บาท")
if water > 20 and water < 31: 
  if meter == "1/2":
    A= (water*F)+30
    B= (A)*tax
    C= B+A       
    print ('ราคาไม่รวมภาษี, %s' %A,"บาท")
    print ('ราคาภาษี, %s' %B,"บาท")
    print ('ราคารวมทั้งหหมด, %s' %C,"บาท")
if water > 30 and water < 50: 
